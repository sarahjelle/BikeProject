<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapp.GUIfx.DockPaneController">
    <top>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField />
                <Button mnemonicParsing="false" text="Search" GridPane.columnIndex="1" />
                <Button mnemonicParsing="false" prefHeight="39.0" prefWidth="158.0" text="+ Register new" GridPane.columnIndex="3" />
                <!-- onAction="#regDockingStation" -->
            </children>
        </GridPane>
    </top>
    <center>
        <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" visible="true">
            <columns>
                <TableColumn fx:id="dockid" prefWidth="60.0" text="DockID" />
                <TableColumn fx:id="dockname" prefWidth="60.0" text="Station name" />
                <TableColumn fx:id="dockcap" prefWidth="60.0" text="Bike capacity" />
                <TableColumn fx:id="dockpower" prefWidth="60.0" text="Power usage" />
                <TableColumn fx:id="dockedit" prefWidth="60.0" text="Edit" />
                <TableColumn fx:id="dockdelete" prefWidth="60.0" text="Delete" />
            </columns>
        </TableView>
    </center>
    <left>
        <Pane prefHeight="20.0" prefWidth="20.0" BorderPane.alignment="CENTER" />
    </left>
    <right>
        <Pane prefHeight="20.0" prefWidth="20.0" BorderPane.alignment="CENTER" />
    </right>
    <bottom>
        <Pane prefHeight="20.0" prefWidth="20.0" BorderPane.alignment="CENTER" />
    </bottom>
</BorderPane>
